{
  "name": "gaego-ng2-skeleton",
  "version": "0.0.0",
  "description": "",
  "main": "index.html",
  "dependencies": {
    "angular2": "^2.0.0-alpha.46"
  },
  "devDependencies": {
    "fs-extra": "^0.26.2",
    "tsconfig-cli": "^0.1.1",
    "tslint": "^3.0.0",
    "typescript": "^1.6.2",
    "typescript-formatter": "^1.1.0",
    "webpack": "^1.12.9"
  },
  "scripts": {
    "start": "live-server --entry-file=index.html",
